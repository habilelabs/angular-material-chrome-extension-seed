{
  "manifest_version": 2,
  "name": "Angular Material Extensionßß",
  "description": "Google chrome extension example with angular material",
  "version": "1.0",
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_popup": "index.html"
  },
  "permissions": [],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "assets/css/*",
    "assets/js/*",
    "assets/fonts/*"
  ]
}